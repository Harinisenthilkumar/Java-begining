//Example-2
//Input : num = 11
//Output : Positive

import java.io.*;
public class Main {
  public static void main (String[]args){
    int num = 11;
    if (num > 0)
        System.out.println ("The number is positive");
    else if (num < 0)
        System.out.println ("The number is negative");
    else
        System.out.println ("Zero");
  }
}
